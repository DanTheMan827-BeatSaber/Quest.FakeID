{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "version": "0.1.0",
  "info": {
    "name": "Fake ID",
    "id": "FakeId",
    "version": "0.1.0",
    "url": null,
    "additionalData": {
      "overrideSoName": "libFakeId.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ],
      "make-hooking": [
        "pwsh ./common-stuff/scripts/make-hooking.ps1"
      ]
    },
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodOutput": "./Fake ID.qmod",
    "ndk": "^27.0.0"
  },
  "dependencies": [
    {
      "id": "bs-cordl",
      "versionRange": "4010.*",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.5.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.0",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    }
  ]
}
